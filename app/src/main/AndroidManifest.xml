<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.letsgo">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Category.CategoryActivity"></activity>
        <activity android:name=".Country.CountryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Reviews.ReviewActivity" />
        <activity android:name=".Place.PlaceActivity" />
        <activity android:name=".ContactUs.ContactUsActivity" />
        <activity android:name=".Splash.Splash" />
        <activity android:name=".Adminstrator.PushingData" />
        <activity android:name=".Registeration.RegisterActivity" />
        <activity android:name=".LogIn.LogInActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>